<header id="header">
  <a href="<%- url_for("/") %>">
  <% if (theme.customize.logo && theme.customize.logo.enabled) { %>
    <div id="logo" style="background-image: url(<%- url_for(theme.customize.logo.url) %>);"></div>
  <% } %>
    <div id="title">
      <h1><%= config.title %></h1>
    </div>
  </a>
  <div id="nav">
    <ul>
      <li class="icon">
        <a href="#"><i class="fa fa-bars fa-2x"></i></a>
      </li>
      <% for (var key in theme.nav) { %> 
        <li>
          <% if (theme.nav[key].startsWith('http')) { %>
            <a href="<%- url_for(theme.nav[key]) %>" rel="noopener"><%= key %></a>
          <% } else { %>
            <a href="<%- url_for(theme.nav[key]) %>"><%= key %></a>
          <% } %>
        </li>
      <% } %>
    </ul>
  </div>
</header>
